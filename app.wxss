/**app.wxss**/
view{
  box-sizing: border-box;
}

page{
  background-color: #f8f8f8;
}

.right-arrow::after{
  content: '';
  position: absolute;
  width: 16rpx;
  height: 16rpx;
  border: 4rpx solid #aaa;
  right: 40rpx;
  top: 50%;
  transform: translateY(-50%) rotate(45deg);
  border-width: 4rpx 4rpx 0 0;
}

.center-center{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
}

.cl{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}

.ct{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}

.flex{
  display: flex;
}

.no-more{
  font-size: 24rpx;
  text-align: center;
  width: 100%;
  background-color: #fff;
  color: #999;
  padding-bottom: 10rpx;
  margin-top: -16rpx;
}

.fix-iphonex-button {
  bottom: 68rpx!important;
}

.fix-iphonex-button::after {
  content: ' ';
  position: fixed;
  bottom: 0!important;
  height: 68rpx!important;
  width: 100%;
  background: #fff;
}

.fixed-icon{
  width:120rpx;
  height:120rpx;
  position:fixed;
  top:940rpx;
  z-index:1;
  right: 5%;
}

.fixed-icon image{
  width:120rpx;
  height:120rpx;
}